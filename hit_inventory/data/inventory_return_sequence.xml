    <record id="inventory_return_sequence" model="ir.sequence">
        <field name="name">Sequence Inventory Return (RET)</field>
        <field name="code">wh.ret.sequence</field>
        <field name="active">TRUE</field>
        <field name="prefix">WH/RET/%(year)s/</field>

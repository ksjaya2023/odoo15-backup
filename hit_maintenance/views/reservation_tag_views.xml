<odoo>
    <data>

        <record id="view_reservation_tag_tree" model="ir.ui.view">
        <field name="name">view.reservation.tag.tree</field>
        <field name="model">reservation.tag</field>
        <field name="type">tree</field>
        <field name="mode">primary</field>
        <field name="priority" eval="16" />
        <field name="active" eval="True" />
        <field name="arch" type="xml">
            <tree string="reservation_tag_tree" create="true" delete="true" edit="true">
                <field name="name"/>
                <field name="create_date" optional="0"/>
                <field name="create_uid" optional="0"/>
            </tree>
        </field>
    </record>
    
    

<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
    
   
    <!-- Tree view for "reservation" -->
    <record id="view_return_request_tree" model="ir.ui.view">
        <field name="name">view.return.request.tree</field>
        <field name="model">return.request</field>
        <field name="type">tree</field>
        <field name="mode">primary</field>
        <field name="priority" eval="16" />
        <field name="active" eval="True" />
        <field name="arch" type="xml">
            <tree string="reservation_tree" create="true" delete="true" edit="true">
                <field name="name"/>
                <field name="transfer_id" />
            </tree>
        </field>
    </record>
    
    <!-- Form view for "model" -->
    <record id="view_return_request_form" model="ir.ui.view">
        <field name="name">view.return.request.form</field>
        <field name="model">return.request</field>
        <field name="type">form</field>
        <field name="mode">primary</field>
        <field name="priority" eval="16" />
        <field name="active" eval="True" />
        <field name="arch" type="xml">
            <form string="reservation_form" create="true" delete="true" edit="true">
                <header>
                </header>
                <sheet>
                    <div class="oe_title">
                        <label for="name" class="oe_edit_only" />
                        <h1>
                            <field name="name" class="oe_inline"
                                placeholder="name" readonly="1"/>
                        </h1>
                    </div>
                    <group>
                        <group>
                            <field name="reservation_id" />
                            <field name="equipment_id" />
                            <field name="work_order_id" />
                            <field name="return_document_id" />
                        </group>
                        <group>
                            <field name="create_date" />
                            <field name="create_uid" />
                        </group>
                    </group>
                    <notebook>
                        <page name="details" string="Return Product">
                            <field name="return_request_ids">
                                <tree editable="bottom">
                                    <field name="product_id" />
                                    <field name="quantity" />
                                    <field name="uom_id" />
                                    <field name="description" />
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
    
